import java.util.Scanner;
class Main {
    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int count=0;
        while(n>0){
            count=count+1;
            n=n/10;
        }
        System.out.println(count);
        
    }
    
}

/*Testcase 1 :

Input : 

325345

Expected output:

6



Testcase 2 :

Input : 

9879

Expected output:

4 */
